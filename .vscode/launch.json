{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/conan_app_launcher/__main__.py",
            "env": {
                "CAL_DEBUG_LEVEL": "1"
            },
            "args": ["--debug"],
            "console": "integratedTerminal",
            "subProcess": true,
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "subProcess": true,
        },
